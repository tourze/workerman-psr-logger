{
  "name": "tourze/workerman-psr-logger",
  "description": "A PSR Logger integrate with Workerman::logger() function",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "clue/hexdump": "0.2.*",
    "psr/log": "^3|^2|^1",
    "workerman/workerman": "^5.1",
    "yiisoft/json": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\PsrLogger\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\PsrLogger\\Tests\\": "tests"
    }
  }
}